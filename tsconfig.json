{
	"compilerOptions": {
    "jsx": "react",
    "jsx": "react",
    "jsx": "react",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"target": "es2016",
		"module": "ES2022",
		"moduleResolution": "node",
		"outDir": "dist",
		"importHelpers": true,
		"typeRoots": ["./node_modules/@types", "./types"],
		"resolveJsonModule": true,
	},
	"include": ["src/**/*"]
}
